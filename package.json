{
  "name": "repos",
  "version": "1.0.11",
  "private": true,
  "description": "An explorer app for Facebook projects in Github.",
  "author": "Leonardo Quixada <lquixada@gmail.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "prestart": "npm run -s build",
    "prelambda:deploy": "run-s build:babel webpack:client",
    "eb:build": "docker build -t $npm_package_docker_registry/repos:$npm_package_version .",
    "eb:login": "eval $(aws ecr get-login --no-include-email --region us-east-1)",
    "eb:pack": "./tasks/eb-pack",
    "eb:push": "docker push $npm_package_docker_registry/repos:$npm_package_version",
    "build": "run-s build:babel webpack",
    "build:analyze": "./tasks/analyze",
    "build:babel": "babel -D src -d web",
    "deploy:major": "npm version major && git push --follow-tags",
    "deploy:minor": "npm version minor && git push --follow-tags",
    "deploy:patch": "npm version patch && git push --follow-tags",
    "dev": "nodemon src/app.js",
    "lambda:deploy": "serverless deploy -v",
    "lambda:start": "serverless offline --skipCacheInvalidation",
    "start": "NODE_ENV=production pm2 start .process.yml --no-daemon",
    "ssl:build": "./tasks/ssl",
    "test": "run-s test:jest test:lint",
    "test:codecov": "codecov",
    "test:jest": "jest --coverage",
    "test:lint": "standard",
    "webpack": "run-s webpack:client webpack:server",
    "webpack:client": "webpack-cli -p --env.prod --env.client",
    "webpack:server": "webpack-cli -p --env.prod --env.server"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "env": "jest"
  },
  "docker": {
    "registry": "328217091811.dkr.ecr.us-east-1.amazonaws.com"
  },
  "dependencies": {
    "aws-serverless-express": "3.2.0",
    "babel-polyfill": "6.26.0",
    "body-parser": "1.18.3",
    "compression": "1.7.2",
    "cross-fetch": "2.2.2",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "helmet": "3.12.1",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "morgan": "1.9.0",
    "pm2": "3.0.0",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-component-octicons": "1.6.0",
    "react-dom": "16.4.1",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.3",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-saga": "1.0.0-beta.1",
    "styled-components": "3.3.3",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.52",
    "@babel/core": "7.0.0-beta.52",
    "@babel/node": "7.0.0-beta.52",
    "@babel/preset-env": "7.0.0-beta.52",
    "@babel/preset-react": "7.0.0-beta.52",
    "assets-webpack-plugin": "3.8.4",
    "babel-core": "^7.0.0-0",
    "babel-jest": "23.2.0",
    "babel-loader": "8.0.0-beta.3",
    "clean-webpack-plugin": "0.1.19",
    "codecov": "3.0.3",
    "copy-webpack-plugin": "4.5.2",
    "dotenv-webpack": "1.5.7",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.4",
    "flux-standard-action": "2.0.3",
    "jest": "23.3.0",
    "lint-staged": "7.2.0",
    "nock": "9.4.1",
    "node-zip": "1.1.1",
    "nodemon": "1.17.5",
    "npm-run-all": "4.1.3",
    "react-test-renderer": "16.4.1",
    "serverless": "1.28.0",
    "serverless-offline": "3.25.6",
    "serverless-s3-deploy": "0.7.0",
    "standard": "11.0.1",
    "supertest": "3.1.0",
    "webpack": "4.15.1",
    "webpack-cli": "3.0.8",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/repos/"
  }
}
