{
  "name": "repos",
  "version": "3.1.8",
  "private": true,
  "description": "An explorer app for projects in Github.",
  "author": "Leonardo Quixada <lquixada@gmail.com>",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "prelambda:deploy": "npm run -s build",
    "preprod": "npm run -s build",
    "build": "npm run -s build:babel && npm run -s build:webpack",
    "build:analyze": "./tasks/analyze",
    "build:babel": "babel -D src -d dist",
    "build:webpack": "webpack-cli -p --env.prod",
    "deploy:major": "npm version major && git push --follow-tags",
    "deploy:minor": "npm version minor && git push --follow-tags",
    "deploy:patch": "npm version patch && git push --follow-tags",
    "dev": "pm2 start --no-daemon",
    "lambda:deploy": "serverless deploy -v",
    "prod": "NODE_ENV=production serverless offline --noEnvironment --skipCacheInvalidation --dontPrintOutput",
    "ssl:build": "./tasks/ssl",
    "test": "npm run -s test:jest && npm run -s test:lint",
    "test:codecov": "codecov",
    "test:jest": "jest --coverage",
    "test:lint": "standard"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "standard": {
    "env": "jest"
  },
  "dependencies": {
    "@octokit/rest": "15.9.4",
    "apollo-boost": "0.1.11",
    "body-parser": "1.18.3",
    "compression": "1.7.3",
    "cross-fetch": "2.2.2",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "graphql-tag": "2.9.2",
    "graphql-yoga": "1.14.12",
    "helmet": "3.13.0",
    "history": "4.7.2",
    "immutable": "3.8.2",
    "morgan": "1.9.0",
    "nprogress": "0.2.0",
    "pm2": "3.0.1",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-github-fork-ribbon": "0.5.1",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.3",
    "react-octicons": "0.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.3.1",
    "redux": "4.0.0",
    "redux-saga": "1.0.0-beta.1",
    "serverless-http": "1.6.0",
    "styled-components": "3.3.3",
    "winston": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.54",
    "@babel/core": "7.0.0-beta.54",
    "@babel/node": "7.0.0-beta.54",
    "@babel/preset-env": "7.0.0-beta.54",
    "@babel/preset-react": "7.0.0-beta.54",
    "apollo-cache-inmemory": "1.2.6",
    "apollo-client": "2.3.6",
    "apollo-link-schema": "1.1.0",
    "assets-webpack-plugin": "3.8.4",
    "babel-core": "^7.0.0-0",
    "babel-jest": "23.4.0",
    "babel-loader": "8.0.0-beta.3",
    "clean-webpack-plugin": "0.1.19",
    "codecov": "3.0.4",
    "copy-webpack-plugin": "4.5.2",
    "dotenv-webpack": "1.5.7",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.4",
    "flux-standard-action": "2.0.3",
    "graphql-tools": "3.0.5",
    "jest": "23.4.1",
    "lint-staged": "7.2.0",
    "nock": "9.4.3",
    "react-test-renderer": "16.4.1",
    "regenerator-runtime": "0.12.0",
    "serverless": "1.28.0",
    "serverless-offline": "3.25.6",
    "serverless-s3-deploy": "0.7.0",
    "standard": "11.0.1",
    "supertest": "3.1.0",
    "webpack": "4.16.2",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lquixada/repos/"
  }
}
